{
  "name": "sk-carousel",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --config jest.config.js",
    "tsc": "npx tsc src/components/index.ts --skipLibCheck --module esnext",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint . --fix --ext .js,.ts,.jsx,.tsx --max-warnings 0  && pretty-quick --branch dev",
    "commit": "git add . && cz-customizable",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "taokepppooo",
  "license": "MT",
  "config": {
    "commitizen": {
      "path": [
        "@commitlint/cz-commitlint",
        "cz-customizable"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "cz-commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{less,postcss,css,scss}": "stylelint --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.7.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
